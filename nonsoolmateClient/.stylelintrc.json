{
  "processors": ["stylelint-processor-styled-components"],
  "extends": ["stylelint-config-styled-components", "stylelint-config-clean-order", "stylelint-config-prettier"],
  "plugins": ["stylelint-order"],
  "overrides": [
    {
      "files": ["*/.{ts,tsx}"],
      "customSyntax": "@stylelint/postcss-css-in-js"
    }
  ],
  "rules": { "no-missing-end-of-source-newline": null }
}
